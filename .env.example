# Firebase Configuration - Environment Variables
# INSTRUCTIONS: Copy this file to .env and fill with YOUR credentials
# DO NOT COMMIT .env file to Git (add to .gitignore)
# Only .env.example should be in version control

# Firebase API Key (found in Firebase Console > Project Settings)
FIREBASE_API_KEY=your_api_key_here

# Firebase App ID (found in Firebase Console > Project Settings)
FIREBASE_APP_ID=your_app_id_here

# Firebase Messaging Sender ID (found in Firebase Console > Project Settings)
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id

# Firebase Project ID (found in Firebase Console > Project Settings)
FIREBASE_PROJECT_ID=your_project_id

# Firebase Auth Domain (usually: project-id.firebaseapp.com)
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com

# Firebase Storage Bucket (usually: project-id.appspot.com)
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
